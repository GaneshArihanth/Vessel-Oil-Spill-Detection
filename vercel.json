{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/final-version/server.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/frontend/dist/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/dist/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/dist/index.html"
        }
    ],
    "builds": [
        {
            "src": "final-version/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ]
}